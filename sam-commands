aws cloudformation package \
   --template-file ./aws-lambda/examplefolder/example.yaml \
   --output-template-file serverless-output.yaml \
   --s3-bucket api-andrews-examplefolder

aws cloudformation deploy \
   --template-file ./serverless-output.yaml \
   --stack-name api-andrews \
  --capabilities CAPABILITY_IAM

awslogs get /aws/lambda/hrm-oauth2Handler-mobilehub-1405917934 --start='20m'

this is the oauth2 function 
